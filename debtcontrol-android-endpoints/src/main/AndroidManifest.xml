<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kalandyk"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="16" />

    <permission android:name="com.kalandyk.permission.C2D_MESSAGE" android:protectionLevel="signature"/>

    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-permission android:name="com.kalandyk.permission.C2D_MESSAGE"/>
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.USE_CREDENTIALS"/>

    <application>
        <service android:name="com.kalandyk.GCMIntentService"/>
        <receiver android:name="com.google.android.gcm.GCMBroadcastReceiver"
                android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE"/>
                <category android:name="com.kalandyk"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.REGISTRATION"/>
                <category android:name="com.kalandyk"/>
            </intent-filter>
        </receiver>
        <activity android:launchMode="singleTop" android:name=".RegisterActivity"/>
    </application>

</manifest>